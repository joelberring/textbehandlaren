{
    "builds": [
        {
            "src": "backend/app/main.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.12"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/app/main.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "backend/app/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "backend/app/main.py"
        }
    ]
}